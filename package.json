{
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "cz": "git-cz",
    "start:components": "pnpm run -C packages/components start",
    "generate:components": "pnpm run -C packages/components generate",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ssr": "vitest --config ./ssr-testing/vitest.config.ts",
    "prepare:e2e": "if [ ! -d \"docs/.vitepress/dist\" ]; then pnpm run docs:build; fi;",
    "dev": "pnpm -C play dev",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "sucrase-node scripts/gen-version.ts",
    "diff:table": "sucrase-node scripts/build-table.ts",
    "update:version": "sucrase-node scripts/update-version.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "build:icons": "pnpm run -C packages/wheel-icons build:icons",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx,.json --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .ts,.tsx && pretty-quick --branch dev",
    "lint:commit": "commitlint --from $(git merge-base dev HEAD) --to HEAD > ./commit-lint.txt",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "docs:gen-locale": "pnpm run -C docs gen-locale",
    "docs:crowdin-credentials": "pnpm run -C docs crowdin-credentials",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky install"
  },
  "dependencies": {
    "@svgr/core": "^5.5.0",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/svgo": "^2.3.1",
    "@wheel-design/components": "workspace:*",
    "@wheel-design/theme-chalk": "workspace:*",
    "camelcase": "^6.2.0",
    "dayjs": "^1.11.1",
    "gulp": "^4.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "svgo": "^2.7.0",
    "svgson": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@pnpm/find-workspace-packages": "^4.0.0",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/node": "*",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "@wheel-design/icons": "workspace:^0.0.1",
    "consola": "^2.15.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-react": "^7.29.4",
    "babel-eslint": "10.1.0",
    "husky": "^7.0.4",
    "jsdom": "16.4.0",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "sucrase": "^3.21.0",
    "ts-morph": "^14.0.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "jsdom": "16.4.0"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
